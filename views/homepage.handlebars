<!-- Loop over each object in the projects array -->
{{#each workout as |workout| }}
<div class="row mb-4 project">
  <div class="col-md-5">
    <h2>
      <!-- Call helper function with triple brackets to render the emoji span as HTML -->
      {{{get_emoji}}}
      <a href="/workout/{{workout.ID}}">{{workout.Workout}}</a>
    </h2>
    <p>
      <span for="img" aria-label="weight">🏋️</span>
      <!-- Pass needed_funding value to the helper function -->
      <ul class="sets_reps">
        <li>{{workout.Sets}}</li>
        <li>{{workout.Reps}}</li>  
      </ul>
    </p> 
  </div>
</div>
{{/each}}